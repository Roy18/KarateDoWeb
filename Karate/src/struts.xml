<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <constant name = "struts.devMode" value = "true" />
    <constant name="struts.enable.DynamicMethodInvocation" value="true"/>

    <package name="default" extends="struts-default">
        <action name="index">
            <result>index.jsp</result>
        </action>
        <action name="dataUser" class="com.glitched.Karatewebprofile.actions.PostAction">
            <result name="success">index.jsp</result>
        </action>
        <action name="register" class="com.glitched.Karatewebprofile.actions.UserAction" method="execute">
            <result name="success">index.jsp</result>
            <result name="input">userRegister.jsp</result>
        </action>
        <action name="dataEvent" class="com.glitched.Karatewebprofile.actions.EventAction">
            <result name="success">main.jsp</result>
        </action>
        <action name="login" class="com.glitched.Karatewebprofile.actions.LoginAction" method="execute">
            <result name="success">main.jsp</result>
            <result name="input">index.jsp</result>
        </action>
        <action name="logout" class="com.glitched.Karatewebprofile.actions.LogoutAction" method="execute">
            <result name="success">index.jsp</result>
        </action>
        <action name="self" class="com.glitched.Karatewebprofile.actions.SelfProfileAction" method="execute">
            <result name="success">profilePage.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="profile" class="com.glitched.Karatewebprofile.actions.ProfileAction" method="execute">
            <result name="success">profilePage.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="home" class="com.glitched.Karatewebprofile.actions.HomeAction" method="execute">
            <result name="success">main.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="createPost" class="com.glitched.Karatewebprofile.actions.PostAction" method="execute">
            <result name="success">main.jsp</result>
            <result name="error">index.jsp</result>
            <result name="input">createPost.jsp</result>
        </action>
        <action name="createEvent" class="com.glitched.Karatewebprofile.actions.EventAction" method="execute">
            <result name="success">main.jsp</result>
            <result name="error">createEvent.jsp</result>
        </action>
        <action name="searchVideo" class="com.glitched.Karatewebprofile.actions.SearchAction" method="execute">
            <result name="success">main.jsp</result>
            <result name="error">main.jsp</result>
        </action>
        <action name="contribution" class="com.glitched.Karatewebprofile.actions.ContributionAction" method="execute">
            <result name="success">contribution.jsp</result>
            <result name="error">main.jsp</result>
        </action>
        <action name="point" class="com.glitched.Karatewebprofile.actions.PointAction" method="execute">
            <result name="success">profilePage.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="redirectCreatePost" class="com.glitched.Karatewebprofile.actions.RedirectAction" method="execute">
            <result name="success">createPost.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="redirectCreateEvent" class="com.glitched.Karatewebprofile.actions.RedirectAction" method="execute">
            <result name="success">createEvent.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="listEvents" class="com.glitched.Karatewebprofile.actions.EventListAction" method="execute">
            <result name="success">listEvent.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="emailer" class="com.glitched.Karatewebprofile.actions.Emailer" method="execute">
            <result name="success">/success.jsp</result>
            <result name="error">/error.jsp</result>
        </action>
        <action name="postDetails" class="com.glitched.Karatewebprofile.actions.PostDetailsAction" method="execute">
            <result name="success">postDetails.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="createComment" class="com.glitched.Karatewebprofile.actions.NewCommentAction" method="execute">
            <result name="success">postDetails.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="delete" class="com.glitched.Karatewebprofile.actions.DeleteVideoAction" method="execute">
            <result name="success">contribution.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="postulant" class="com.glitched.Karatewebprofile.actions.PostulantAction" method="execute">
            <result name="success">listEvent.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="eventPostulants" class="com.glitched.Karatewebprofile.actions.showPostulantsAction" method="execute">
            <result name="success">eventDetails.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="userPostulant" class="com.glitched.Karatewebprofile.actions.RelatedEventsAction" method="execute">
            <result name="success">myEvents.jsp</result>
            <result name="error">index.jsp</result>
        </action>
        <action name="updateProfile" class="com.glitched.Karatewebprofile.actions.updateProfileAction" method="execute">
            <result name="success">profilePage.jsp</result>
            <result name="error">index.jsp</result>
        </action>
    </package>
</struts>